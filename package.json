{
  "name": "budget",
  "version": "1.0.0",
  "scripts": {
    "setup": "yarn install && cd frontend && yarn install && cd .. && cd backend && yarn install && cd ..",
    "dev": "concurrently --kill-others 'yarn run frontend' 'yarn run backend'",
    "frontend": "cd frontend && yarn run start",
    "backend": "cd backend && yarn run start:debug",
    "lintFrontend": "cd frontend && yarn run lint",
    "lintBackend": "cd backend && yarn run lint",
    "lint": "concurrently 'yarn run lintFrontend' 'yarn run lintBackend'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csteven15/budget.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/csteven15/budget/issues"
  },
  "homepage": "https://github.com/csteven15/budget#readme",
  "devDependencies": {
    "@types/node": "^14.14.34",
    "concurrently": "^5.3.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {}
}
