{
  "name": "budget",
  "version": "1.0.0",
  "description": "### To run the application",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others 'npm run devFrontEnd' 'npm run devBackEnd'",
    "devDocker": "docker-compose up --build --force-recreate --no-deps",
    "devFrontEnd": "cd client && npm run start",
    "devBackEnd": "cd server && npm run dev",
    "removeAllImages": "docker images -a -q | xargs docker rmi -f",
    "setup": "cd client && npm install && cd ../server && npm install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/csteven15/budget.git"
  },
  "author": "",
